{
  "name": "clickup-aut-wdio",
  "version": "1.0.0",
  "description": "Clickup automation",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/wdio wdio.conf.js",
    "start-edgedriver": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command Start-Process \"msedgedriver.exe\" -WindowStyle Hidden",
    "stop-edgedriver": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command taskkill /IM msedgedriver.exe",
    "test-report": "allure generate ./allure-results && allure open",
    "config-runner": "./node_modules/.bin/wdio config"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aut-training/clickup-aut-wdio.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Aut-training/clickup-aut-wdio/issues"
  },
  "homepage": "https://github.com/Aut-training/clickup-aut-wdio#readme",
  "devDependencies": {
    "@wdio/allure-reporter": "^5.18.6",
    "@wdio/cli": "^5.16.15",
    "@wdio/cucumber-framework": "^5.16.15",
    "@wdio/firefox-profile-service": "^5.16.11",
    "@wdio/local-runner": "^5.16.15",
    "@wdio/mocha-framework": "^5.16.15",
    "@wdio/spec-reporter": "^5.16.11",
    "@wdio/sync": "^5.16.15",
    "axios": "^0.19.0",
    "chai": "^4.2.0",
    "chromedriver": "^80.0.1",
    "eslint": "^6.7.2",
    "geckodriver": "^1.19.1",
    "wdio-chromedriver-service": "^5.0.2",
    "wdio-edgedriver-service": "^1.0.0",
    "wdio-geckodriver-service": "^1.0.3"
  }
}
